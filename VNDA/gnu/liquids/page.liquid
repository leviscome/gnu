{% head %} 
  {% opengraphfor page %}

  <link rel="stylesheet" type="text/css" href="/pages/institucional.css" inline>
{% endhead %} 

<div>
  {% if page.slug == "onde-encontrar" %} 
    {% render "partials/pages/stores", 
      page: page 
    %} 
  {% elsif page.slug == 'atendimento'  %} 
    {% render "partials/pages/contact", 
      page: page,
      current_shop: current_shop 
    %} 
  {% elsif page.slug == 'sobre'  %} 
    {% render "partials/pages/about", 
      page: page,
      current_shop: current_shop 
    %} 
  {% else %}
    {% render "partials/pages/custom", 
      page: page,
      current_shop: current_shop 
    %} 
  {% endif %}
</div>
